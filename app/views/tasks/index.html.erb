<h1>Listing tasks</h1>
<%= link_to 'New task', new_task_path %>
<table>
  <tr>
    <th>Title</th>
    <th>Description</th>
    <th>Show</th>
    <th>Edit</th>
    <th>Delete</th>
  </tr>

  <% @tasks.each do |task| %>
    <tr>
      <td><%= task.title %></td>
      <td><%= task.desc %></td>
      <td><%= link_to 'Show', task %></td>
      <td><%= link_to 'Edit', edit_task_path(task) %></td>
      <td><%= link_to 'Delete'%></td>
    </tr>
  <% end %>
</table>
